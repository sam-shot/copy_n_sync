{
    "name": "copy_n_sync",
    "version": "1.0",
    "description": "A new Flutter project.",
    "action": {
        "default_popup": "index.html",
        "default_icon": "icons/Icon-512.png"
    },
    "manifest_version": 3,
    "permissions": [
        "clipboardWrite",
        "clipboardRead"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "icons": [
        {
            "src": "icons/Icon-192.png",
            "sizes": "192x192",
            "type": "image/png"
        },
        {
            "src": "icons/Icon-512.png",
            "sizes": "512x512",
            "type": "image/png"
        },
        {
            "src": "icons/Icon-maskable-192.png",
            "sizes": "192x192",
            "type": "image/png",
            "purpose": "maskable"
        },
        {
            "src": "icons/Icon-maskable-512.png",
            "sizes": "512x512",
            "type": "image/png",
            "purpose": "maskable"
        }
    ]
}